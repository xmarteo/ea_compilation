sw    $ra, 16($sp)
sw    $s3, 12($sp)
sw    $s2, 8($sp)
sw    $s1, 4($sp)
sw    $s0, 0($sp)
move  $s2, $a0
move  $s3, $a1
addiu $a0, $s2, -1
li    $v0, 2
div   $v0, $a0, $v0
bltz  $v0, prod56
sll   $v0, $v0, 1
addiu $s0, $v0, 1
prod48:
