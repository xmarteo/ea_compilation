# begin check
check27:
addiu $sp, $sp, -8
sw    $ra, 4($sp)
sw    $s0, 0($sp)
li    $s0, 201
check15:
addiu $a0, $s0, -1
li    $v0, 2
div   $a0, $a0, $v0
bgez  $a0, check42
lw    $ra, 4($sp)
