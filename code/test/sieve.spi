sieve16:
sll   $v0, $s2, 2
addu  $v0, $s0, $v0
lw    $v0, 0($v0)
bgtz  $v0, sieve60
sieve2:
addiu $s2, $s2, 1
j     sieve18
sieve60:
move  $a0, $s2
jal   writeln
sll   $a0, $s2, 1
sieve10:
slt   $v0, $a0, $s1
blez  $v0, sieve2
sll   $v0, $a0, 2
addu  $v0, $s0, $v0
sw    $zero, 0($v0)
