addiu $a1, $t4, -1
li    $a0, 2
div   $a0, $a1, $a0
addu  $v0, $v0, $a0
sll   $v0, $v0, 1
addiu $v0, $v0, 1
addiu $sp, $sp, 24
jr    $ra
# end f

# begin _main
_main50:
addiu $sp, $sp, -32
sw    $ra, 28($sp)
li    $v0, 3
sw    $v0, 0($gp)
lw    $v0, 0($gp)
addiu $a0, $v0, -1
li    $v0, 2
div   $v0, $a0, $v0
sll   $v0, $v0, 1
addiu $v1, $v0, 1
sw    $v1, 0($sp)
li    $a0, 5
jal   g17
addiu $a0, $v0, -1
li    $v0, 2
div   $v0, $a0, $v0
sll   $v0, $v0, 1
addiu $v1, $v0, 1
sw    $v1, 4($sp)
li    $v1, 7
sw    $v1, 8($sp)
li    $v1, 9
sw    $v1, 12($sp)
li    $v1, 11
sw    $v1, 16($sp)
li    $v1, 13
sw    $v1, 20($sp)
li    $v1, 15
sw    $v1, 24($sp)
li    $a0, 17
jal   g17
addiu $a0, $v0, -1
li    $v0, 2
div   $v0, $a0, $v0
sll   $v0, $v0, 1
addiu $a0, $v0, 1
jal   g17
addiu $a0, $v0, -1
li    $v0, 2
div   $v0, $a0, $v0
sll   $v0, $v0, 1
addiu $t1, $v0, 1
li    $t0, 19
li    $v0, 21
lw    $a0, 0($sp)
lw    $a1, 4($sp)
lw    $a2, 8($sp)
lw    $a3, 12($sp)
lw    $v1, 16($sp)
sw    $v1, -4($sp)
lw    $v1, 20($sp)
sw    $v1, -8($sp)
lw    $v1, 24($sp)
sw    $v1, -12($sp)
sw    $t1, -16($sp)
sw    $t0, -20($sp)
sw    $v0, -24($sp)
jal   f62
addiu $a0, $v0, -1
li    $v0, 2
div   $a0, $a0, $v0
lw    $ra, 28($sp)
addiu $sp, $sp, 32
j     writeln
# end _main
