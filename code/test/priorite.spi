li    $v0, 1
bgtz  $v0, _main5
li    $v0, 1
bgtz  $v0, _main5
_main2:
li    $a0, 0
_main60:
lw    $ra, 0($sp)
addiu $sp, $sp, 4
j     writeln
_main5:
blez  $zero, _main2
li    $a0, 1
