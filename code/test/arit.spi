lw    $s1, 4($sp)
lw    $s0, 0($sp)
addiu $sp, $sp, 20
j     writeln
# end coucou

# begin _main
_main20:
addiu $sp, $sp, -4
sw    $ra, 0($sp)
jal   readln
sw    $v0, 0($gp)
_main8:
lw    $a0, 0($gp)
bne   $a0, $zero, _main34
lw    $ra, 0($sp)
addiu $sp, $sp, 4
jr    $ra
_main34:
